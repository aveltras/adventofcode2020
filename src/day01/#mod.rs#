use std::fs;
use std::str::Lines;

pub fn solve() {
    let contents = fs::read_to_string("input.txt").unwrap();
    let lines_iter = contents.lines();

    for first in lines_iter.i {
        for second in lines_iter {
            let first_operand: u8 = first.parse().unwrap();
            let second_operand: u8 = second.parse().unwrap();
            if first_operand + second_operand == 2020 {
                break;
            }
        }
    }
}
